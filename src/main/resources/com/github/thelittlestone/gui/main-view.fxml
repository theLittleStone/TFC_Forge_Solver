    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <Button alignment="CENTER" onAction="#onHelloButtonClick" text="Hello!" textAlignment="CENTER" />
</VBox>
